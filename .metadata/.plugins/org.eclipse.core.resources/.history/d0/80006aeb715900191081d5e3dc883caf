#include "Bibliographie.h"
using namespace std;
using namespace biblio;

string Bibliographie::reqBibliographieFormate() const{
	ostringstream os;
	os << "Bibliographie" << endl << "===============================" << endl;
	for (int i = 0; i < m_vReferences.size(); i++){
		os << "[" << i << "]" <<  m_vReferences[i]->reqReferenceFormate()<< "\n";
	}
}
