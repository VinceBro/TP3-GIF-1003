#include "Journal.h"
using namespace std;

std::string Journal::reqReferenceFormate() const{
	ostringstream os;
	os << m_auteurs << ". " << m_titre << ". "<< m_nom << ", vol. " <<
			m_volume << ", no. " << m_numero << ", pp. " << m_page <<
			", " << m_annee << ". " << m_identifiant;
	return os.str();
}

