#include "Ouvrage.h"
using namespace std;
Ouvrage::Ouvrage(const string auteurs, const std::string titre, const std::string identifiant, const int annee, const std::string editeur, const std::string ville)
{
	m_auteurs = auteurs;
	m_titre = titre;
	m_identifiant = identifiant;
	m_annee = annee;
	m_editeur = editeur;
	m_ville = ville;
}

biblio::Reference* Ouvrage::clone() const{
	return new Ouvrage(*this);
}
